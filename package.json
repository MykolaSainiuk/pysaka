{
  "name": "pysaka",
  "version": "1.0.9",
  "description": "Async threaded logger",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "prebuild": "rimraf ./dist/*.js && rimraf ./dist/tests/*.js",
    "test:only": "npm run build && node --test-only ./dist/tests/index.js",
    "test": "npm run build && node --test ./dist/tests/index.js",
    "test:cov": "npm run build && node --test --experimental-test-coverage --test-reporter=spec ./dist/tests/index.js",
    "lint": "eslint --ext .ts .",
    "format": "prettier --write \"./**/*.ts\"",
    "build": "tsc -p ./tsconfig.json",
    "start": "node ./dist/index.js",
    "dev": "ts-node ./src",
    "prepublish": "npm run build"
  },
  "keywords": [
    "nodejs",
    "logger",
    "async",
    "logger",
    "threads",
    "pysaka"
  ],
  "homepage": "https://github.com/MykolaSainiuk/pysaka/blob/master/README.md",
  "author": "Mykola Sainiuk",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^20.8.7",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.1",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2"
  }
}
